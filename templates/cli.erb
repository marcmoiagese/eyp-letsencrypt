rsa-key-size = <%= @rsakeysize %>
<% if defined?(@email) -%>
email = <%= @email %>
<% else -%>
register-unsafely-without-email
<% end -%>
agree-tos = <%= @agree_tos %>
renew-by-default = <%= @autorenew %>
non-interactive = True

# authenticator specific

authenticator = webroot
webroot-path = <%= @webroot %>
